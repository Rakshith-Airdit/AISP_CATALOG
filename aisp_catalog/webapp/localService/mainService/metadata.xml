                <Property Name="CreatedAt" Type="Edm.DateTimeOffset" sap:label="Created At" sap:quickinfo="Timestamp when record was created"/>
                <Property Name="CreatedBy" Type="Edm.String" MaxLength="50" sap:label="Created By" sap:quickinfo="User who created the record"/>
                <Property Name="ModifiedAt" Type="Edm.DateTimeOffset" sap:label="Modified At" sap:quickinfo="Timestamp when record was last modified"/>
                <Property Name="ModifiedBy" Type="Edm.String" MaxLength="50" sap:label="Modified By" sap:quickinfo="User who last modified the record"/>
