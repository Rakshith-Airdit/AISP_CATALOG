                <Property Name="CommodityCode" Type="Edm.Int32" sap:label="Commodity Code" sap:quickinfo="UNSPSC commodity classification code"/>
                <Property Name="Category" Type="Edm.String" MaxLength="20" sap:label="Category" sap:quickinfo="UNSPSC commodity classification code"/>
                <Property Name="SearchTerm" Type="Collection(Edm.String)" Nullable="false" sap:label="Search Term" sap:quickinfo="Keywords for product search"/>
                <Property Name="UnitPrice" Type="Edm.Decimal" Precision="15" Scale="2" sap:label="Unit Price" sap:quickinfo="Price per unit of the product"/>
                <Property Name="CurrencyCode" Type="Edm.String" MaxLength="3" sap:label="Currency Code" sap:quickinfo="Currency code for pricing"/>
                <Property Name="UnitOfMeasure" Type="Edm.String" MaxLength="10" sap:label="Unit of Measure" sap:quickinfo="Measurement unit for the product"/>
                <Property Name="LeadTimeDays" Type="Edm.Int32" sap:label="Lead Time" sap:quickinfo="Delivery lead time in days"/>
