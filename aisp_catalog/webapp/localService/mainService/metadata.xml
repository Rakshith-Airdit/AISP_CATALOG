            <EntityType Name="ProductCatalogItems">
                <Key>
                    <PropertyRef Name="ProductId"/>
                </Key>
                <Property Name="ProductId" Type="Edm.String" Nullable="false" sap:label="Product ID" sap:quickinfo="Unique identifier for the product"/>
